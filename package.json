{
  "version": "0.0.12",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "changeset": "changeset",
    "version": "changeset version && npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "moyan-utils",
  "author": "moyan",
  "module": "dist/moyan-utils.esm.js",
  "size-limit": [
    {
      "path": "dist/moyan-utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/moyan-utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nangongmoyan/moyan-utils.git"
  },
  "bugs": {
    "url": "https://github.com/nangongmoyan/moyan-utils/issues"
  },
  "homepage": "https://github.com/nangongmoyan/moyan-utils#readme",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@size-limit/preset-small-lib": "^8.2.4",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^3.9.10"
  }
}
